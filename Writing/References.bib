@article {Sanderson448,
	author = {Sanderson, Michael J. and McMahon, Michelle M. and Steel, Mike},
	title = {Terraces in Phylogenetic Tree Space},
	volume = {333},
	number = {6041},
	pages = {448--450},
	year = {2011},
	doi = {10.1126/science.1206357},
	publisher = {American Association for the Advancement of Science},
	abstract = {A key step in assembling the tree of life is the construction of species-rich phylogenies from multilocus{\textemdash}but often incomplete{\textemdash}sequence data sets. We describe previously unknown structure in the landscape of solutions to the tree reconstruction problem, comprising sometimes vast {\textquotedblleft}terraces{\textquotedblright} of trees with identical quality, arranged on islands of phylogenetically similar trees. Phylogenetic ambiguity within a terrace can be characterized efficiently and then ameliorated by new algorithms for obtaining a terrace{\textquoteright}s maximum-agreement subtree or by identifying the smallest set of new targets for additional sequencing. Algorithms to find optimal trees or estimate Bayesian posterior tree distributions may need to navigate strategically in the neighborhood of large terraces in tree space.},
	issn = {0036-8075},
	URL = {http://science.sciencemag.org/content/333/6041/448},
	eprint = {http://science.sciencemag.org/content/333/6041/448.full.pdf},
	journal = {Science}
}

